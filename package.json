{
  "name": "node-setup",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Pedro Henrique <pedrotrab2132@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/server.js",
    "dev": "dotenv -e .env.dev -- ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify --exit-child src/server.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "migrate:ci": "dotenv -e .env.ci -- npx prisma migrate dev --name postgres-init",
    "migrate:dev": "dotenv -e .env.dev -- prisma migrate dev",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate dev --name postgres-init",
    "migrate:deploy": "prisma migrate deploy",
    "test": "yarn docker:up && yarn migrate:test && dotenv -e .env.test -- jest --runInBand --forceExit --detectOpenHandles || yarn docker:down",
    "test:we": "NODE_ENV=development jest --runInBand",
    "test:ci": "dotenv -e .env.ci -- jest --runInBand --forceExit --detectOpenHandles",
    "test:unit": "yarn docker:up && yarn migrate:test && dotenv -e .env.test -- jest src/modules/users/useCases/User/ChangePassword/ChangePasswordController.spec.ts --runInBand --forceExit --detectOpenHandles || yarn docker:down",
    "prisma:studio": "dotenv -e .env.dev -- prisma studio --browser none",
    "prisma:seed:dev": "dotenv -e .env.dev -- prisma db seed",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && node_modules/.bin/cz --hook || true"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.6",
    "@types/jest": "^27.4.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.0.0-pre.49",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@prisma/client": "^3.9.2",
    "@types/bcryptjs": "^2.4.2",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.23.0",
    "dotenv": "^14.2.0",
    "dotenv-cli": "^4.1.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "prisma": "^3.9.2",
    "qrcode-terminal": "^0.12.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.0.1",
    "socket.io-client": "^4.0.1",
    "supertest": "^6.2.2",
    "tsyringe": "^4.5.0",
    "uuid": "^8.3.2",
    "venom-bot": "^5.0.0",
    "whatsapp-web.js": "^1.19.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
